{
  "name": "backend",
  "version": "1.0.0",
  "description": "Api Financeira",
  "license": "ISC",
  "author": "Anderson Reis",
  "type": "commonjs",
  "main": "src/server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --coverage --detectOpenHandles",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "currently": "^0.0.8",
    "dotenv": "^16.4.7",
    "express": "^4.22.1",
    "jsonwebtoken": "^9.0.3",
    "mysql2": "^3.12.0",
    "sequelize": "^6.37.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "jest": "^29.7.0",
    "nodemon": "^2.0.7",
    "supertest": "^7.1.4"
  },
  "private": true,
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/__tests__/setup.js"
    ]
  }
}
